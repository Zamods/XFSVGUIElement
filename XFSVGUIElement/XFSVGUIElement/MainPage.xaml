<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XFSVGUIElement.MainPage"
             Title="XFSVGUIElement">

    <StackLayout HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand" Padding="20">
        <Button x:Name="ded" Text="Few Examples Demo" Command="{Binding ButtonClicked}" CommandParameter="FewExamplesDemo"/>
        <Button Text="Scaling SVG Demo" Command="{Binding ButtonClicked}" CommandParameter="ScaleDemo"/>
        <Button Text="DropShadow Demo" Command="{Binding ButtonClicked}" CommandParameter="DropShadowDemo"/>
    </StackLayout>

</ContentPage>
